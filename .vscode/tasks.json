{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Expo Web",
			"type": "shell",
			"command": "npx",
			"args": [
				"expo",
				"start",
				"--web"
			],
			"isBackground": true,
			"problemMatcher": [],
			"group": "build"
		},
		{
			"label": "Expo Web (clear)",
			"type": "shell",
			"command": "npx",
			"args": [
				"expo",
				"start",
				"--web",
				"--clear"
			],
			"isBackground": true,
			"problemMatcher": [],
			"group": "build"
		},
		{
			"label": "Start Expo Dev Server",
			"type": "shell",
			"command": "npx expo start --web --port 8083",
			"isBackground": true,
			"problemMatcher": [
				"$tsc"
			],
			"group": "build"
		}
	]
}